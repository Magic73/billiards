{
  "name": "billiards",
  "description": "Billiards",
  "version": "0.2.0",
  "author": "tailuge",
  "main": "dist/main.js",
  "license": "GPL-3.0",
  "scripts": {
    "test": "nyc mocha --require ts-node/register -r jsdom-global/register ./test/*.spec.ts",
    "watch": "gulp",
    "prettify": "prettier --write --no-semi {src,test}/*"
  },
  "dependencies": {
    "@types/three": "^0.92.18",
    "three": "^0.95.0",
    "three-orbitcontrols": "^2.1.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "gulp": "^3",
    "gulp-mocha": "^6.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3",
    "jsdom": "12.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.14.0",
    "sinon": "^6.1.5",
    "ts-node": "^7.0.1",
    "tsify": "^4.0.0",
    "typescript": "^2.9.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0"
  },
  "nyc": {
    "include": [
      "./src/*.ts"
    ],
    "exclude": [
      ".test/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
